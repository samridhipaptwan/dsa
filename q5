#include<stdio.h>
int main(){

    int arr1[15]={
        11,14,18,21,99,22
    };
    int arr2[5]={
        6,7,8,12,14
    };
    for (int i=0;i<=(sizeof(arr2)/sizeof(int)-1);i++){
        arr1[sizeof(arr1)/sizeof(int)+i]=arr2[i];
    }
    for (int i=0;i<=(sizeof(arr1)/sizeof(int)-1);i++){
        printf("%d",arr1[i])
    }
}
