#include<stdio.h>
#include<stdbool.h>
int main(){
    char check = false;
    char num;
    printf("enter a number :-");
    scanf("%c",&num);
    char arr[10]={'a','b','c','e','g','s','w','h','p','o'};
    for (int i=0;i<(sizeof(arr)/sizeof(char));i++){
        if(arr[i]==num){
            printf(" founded at index is:-%d",i);
            chack=true;
            break;
        }
    }
    if(chack==false){
        printf(" not founded in array ");
    }
}
